        "/auth/register": {
            "post": {
                "summary": "Register the user",
                "description": "",
                "deprecated": false,
                "responses": {
                    "201": {
                        "description": "return created user",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/User"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "(Unauthorized) Invalid or missing Access Token"
                    }
                },
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "birthyear",
                                    "email",
                                    "pays",
                                    "username",
                                    "password"
                                ],
                                "properties": {
                                    "nom": {
                                        "type": "string",
                                        "nullable": true
                                    },
                                    "prenom": {
                                        "type": "string",
                                        "nullable": true
                                    },
                                    "birthyear": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email"
                                    },
                                    "telephone": {
                                        "type": "string",
                                        "nullable": true
                                    },
                                    "pays": {
                                        "type": "string"
                                    },
                                    "username": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    },
                                    "photo": {
                                        "type": "string",
                                        "nullable": true,
                                        "format": "binary"
                                    }
                                }
                            }
                        }
                    }
                },
                "tags": [
                    " Auth "
                ]
            }
        },
        "/auth/login": {
            "post": {
                "summary": "Log in the user",
                "description": "",
                "deprecated": false,
                "responses": {
                    "200": {
                        "description": "object of two keys, \"token\" (string) & \"user\" (User)",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "required": [
                                        "token",
                                        "user",
                                        "type"
                                    ],
                                    "properties": {
                                        "user": {
                                            "type": "object",
                                            "$ref": "#/components/schemas/User"
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "30|0O35Eid4cZLr643EUtd2G1C5UkhuuSXnQNpCTAl9"
                                        },
                                        "type": {
                                            "type": "string",
                                            "example": "Bearer"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "(Unauthorized) Invalid or missing Access Token"
                    }
                },
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "tags": [
                    " Auth "
                ]
            }
        },
