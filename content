        "/auth/social/authenticate": {
            "post": {
                "summary": "Log in the user using email or social provider",
                "description": "",
                "deprecated": false,
                "responses": {
                    "200": {
                        "description": "User logged in, \"token\" (string) & \"user\" (User)",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "required": [
                                        "token",
                                        "user",
                                        "type"
                                    ],
                                    "properties": {
                                        "user": {
                                            "type": "object",
                                            "$ref": "#/components/schemas/User"
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "79|S65MctylLZY6ghe1QpA32r6n75smN2CEP362lCqu"
                                        },
                                        "type": {
                                            "type": "string",
                                            "example": "Bearer"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "201": {
                        "description": "New user just logged in for the first time, \"token\" (string) & \"user\" (User)",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "required": [
                                        "token",
                                        "user",
                                        "type"
                                    ],
                                    "properties": {
                                        "user": {
                                            "type": "object",
                                            "$ref": "#/components/schemas/User"
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "87|krvasHWywVrn4q5OC1z3advpN7Tc5FcuizA0jTCq"
                                        },
                                        "type": {
                                            "type": "string",
                                            "example": "Bearer"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "this account has been blocked"
                    },
                    "424": {
                        "description": "Can't retreive user infos on social provider"
                    },
                    "401": {
                        "description": "(Unauthorized) Invalid or missing Access Token"
                    }
                },
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "google__access_token": {
                                        "type": "string"
                                    },
                                    "facebook__access_token": {
                                        "type": "string"
                                    },
                                    "country_code": {
                                        "type": "string",
                                        "nullable": true
                                    },
                                    "location_lon": {
                                        "type": "number",
                                        "nullable": true
                                    },
                                    "location_lat": {
                                        "type": "number",
                                        "nullable": true
                                    }
                                }
                            }
                        }
