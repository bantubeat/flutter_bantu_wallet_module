            "post": {
                "summary": "Add a withdrawal request",
                "description": "",
                "deprecated": false,
                "responses": {
                    "201": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/WithdrawalRequest"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Kyc not validated"
                    },
                    "404": {
                        "description": "Payment preference not found"
                    },
                    "400": {
                        "description": "Bad or expired payment slip"
                    },
                    "406": {
                        "description": "Insufficient amount"
                    },
                    "409": {
                        "description": "Already made a withdrawal this month or pending one exists"
                    },
                    "412": {
                        "description": "Bad or expired OTP code"
                    },
                    "425": {
                        "description": "Withdraw request must be between the 25th and 31th of month"
                    },
                    "401": {
                        "description": "(Unauthorized) Invalid or missing Access Token"
                    }
                },
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "otp_code",
                                    "amount",
                                    "payment_slip",
                                    "payment_preference_uuid"
                                ],
                                "properties": {
                                    "otp_code": {
                                        "type": "string"
                                    },
                                    "amount": {
                                        "type": "number"
                                    },
                                    "payment_slip": {
                                        "type": "string"
                                    },
                                    "payment_preference_uuid": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ],
                "tags": [
                    " Withdrawal "
                ]
            }
        },
        "/balance/withdrawal/requests": {
            "get": {
                "summary": "Get user withdrawal requests",
                "description": "",
                "deprecated": false,
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "required": [
                                        "current_page",
                                        "data",
                                        "first_page_url",
                                        "last_page",
                                        "last_page_url",
                                        "path",
                                        "per_page",
                                        "total"
                                    ],
                                    "properties": {
                                        "current_page": {
                                            "type": "integer",
                                            "example": 2
                                        },
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "$ref": "#/components/schemas/WithdrawalRequest"
                                            }
                                        },
                                        "first_page_url": {
                                            "type": "string",
                                            "example": "http://localhost:8000/api/balance/withdrawal/requests?page=1"
                                        },
                                        "from": {
                                            "type": "integer",
                                            "example": 16
                                        },
                                        "last_page": {
                                            "type": "integer",
                                            "example": 10
                                        },
                                        "last_page_url": {
                                            "type": "string",
                                            "example": "http://localhost:8000/api/balance/withdrawal/requests?page=10"
                                        },
                                        "next_page_url": {
                                            "type": "string",
                                            "example": "http://localhost:8000/api/balance/withdrawal/requests?page=3"
                                        },
                                        "path": {
                                            "type": "string",
                                            "example": "http://localhost:8000/api/balance/withdrawal/requests"
                                        },
                                        "per_page": {
                                            "type": "integer",
                                            "example": 15
                                        },
                                        "prev_page_url": {
                                            "type": "string",
                                            "example": "http://localhost:8000/api/balance/withdrawal/requests?page=1"
                                        },
                                        "to": {
                                            "type": "integer",
                                            "example": 30
                                        },
                                        "total": {
                                            "type": "integer",
                                            "example": 150
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "(Unauthorized) Invalid or missing Access Token"
                    }
                },
                "parameters": [
                    {
                        "in": "query",
                        "name": "limit",
                        "description": "",
                        "required": false,
                        "schema": {
                            "type": "number"
                        }
                    },
                    {
                        "in": "query",
                        "name": "page",
                        "description": "",
                        "required": false,
                        "schema": {
                            "type": "number"
                        }
                    }
                ],
                "security": [
                    {
                        "bearerAuth": []
                    }
                ],
                "tags": [
                    " Withdrawal "
                ]
            }
        },
