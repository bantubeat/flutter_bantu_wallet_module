                        "example": 1731765677
                    },
                    "user": {
                        "type": "object",
                        "$ref": "#/components/schemas/User"
                    }
                },
                "required": [
                    "user_id",
                    "amount",
                    "status",
                    "details",
                    "account_type",
                    "payment_slip"
                ]
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    }
}